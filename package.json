  "repository": {
    "type": "git",
    "url": "git+https://github.com/murongg/hooks-date.git"
  },
  "funding": "https://github.com/sponsors/murongg",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
