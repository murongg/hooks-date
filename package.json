      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rimraf dist && unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "fix": "eslint src/**/*.*  --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.20.2",
    "@babel/types": "^7.17.0",
    "@types/node": "^17.0.23",
    "@vue/test-utils": "^2.0.0-rc.18",
    "bumpp": "^7.1.1",
    "eslint": "^8.13.0",
    "esno": "^0.14.1",
    "jsdom": "^19.0.0",
    "pkgroll": "^1.0.4",
    "pnpm": "^6.32.6",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vite": "^2.9.1",
    "vitest": "^0.9.3"
  }
